.box .one{
  width: 95%;
  height: 200rpx;
  background-color: white;
  margin: 0 auto;
  position: relative;
  box-shadow: 2px 2px 5px 0px #ccc;
  margin-top: 40rpx;
}
.box .one .left{
  width: 200rpx;
  height: 200rpx;
  float: left;
}
.box .one .left image{
  width: 200rpx;
  height: 200rpx;
}
.box .one .right{
  float: left;
}
.box .one .right .tit{
  font-weight: bold;
  position: absolute;
  top: 10rpx;
}
.box .one .right .yiyue{
  position: absolute;
  bottom: 10rpx;
  color: rgb(112, 112, 112);
}
.box .one .right .onyuye{
  position: absolute;
  bottom: 10rpx;
  right: 10rpx;
  border-radius: 30px;
  width: 250rpx;
  height: 60rpx;
  line-height: 60rpx;
  color: white;
  background-color: #a8874f;
  text-align: center;
}
.end{
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 50rpx;
}
